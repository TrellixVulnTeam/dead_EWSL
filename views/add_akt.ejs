<link rel="stylesheet" type="text/css" href="css/profile.css">
<div class="login">
  <div class="login-body">
    <h3 class="login-heading">Формирование Акта Работ</h3>
    <hr>
    <div class="login-form">
      <div class="form-group">
        <ul class="list-inline">
          <li>
            <small>Персональная информация</small>
          </li>
        </ul>
      </div>
      <div class="form-group">
        <ul class="col-sm-5">
            <label for="date">Дата: </label>
            <input id="date" class="form-control" type="date">
            <br><label>Учёная Степень: </label>
            <select class="form-control" id="degree"></select>
            <br><label>Группа: </label>
            <select class="form-control" id="groups"></select>
            <br><label>Предмет: </label>
            <select class="form-control" id="items"></select>
            <br><label>Часы: </label>
            <select class="form-control" id="time">
              <option value="0"> 8:00 - 9:30 </option>
              <option value="1"> 9:40 - 11:10 </option>
              <option value="2"> 11:20 - 12:50 </option>
              <option value="3"> 13:00 - 14:30 </option>
              <option value="4"> 14:40 - 16:10 </option>
            </select>
            <br><label>Вид работы: </label>
            <select class="form-control" id="type_work">
              <option value="0">Лекция</option>
              <option value="1">Практическое занятие</option>
            </select>
            <br><label>Трудовой договор: </label>
            <select class="form-control" id="contracts"></select>



          <br>
          <hr>
          <br>
          <div class="btn btn-success btn-block" id="add_contract">Создать Акт Работ</div>
          <br>
        </ul>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript" src="js/add_akt.js"></script>
